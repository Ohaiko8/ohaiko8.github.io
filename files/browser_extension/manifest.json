{
    "name": "Screenshot",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
        "default_icon": "icon/icon.png",
        "default_popup": "icon/panel.html"
    },
    "permissions": ["storage"],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["jquery.js", "screen.js", "background.js"],
            "run_at": "document_end"
        }
    ]
}